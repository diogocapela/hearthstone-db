{{> header}}
{{> nav}}
<div class="container">

{{> pub_header}}

	<div class="page-header">
		<h1><b>Hearthstone DB</b></h1>
	</div>

	<table id="table-list" class="table table-striped">
		<thead>
			<tr>
				<th>Name</th>
				<th>Set</th>
				<th>Type</th>
				<th>Rarity</th>
				<th>Class</th>
				<th>Cost</th>
				<th>Attack</th>
				<th>Health</th>
			</tr>
		</thead>
		<tbody>

			{{#each Basic}}
			{{> _index}}
			{{/each}}

			{{#each Classic}}
			{{> _index}}
			{{/each}}

			{{#each Promo}}
			{{> _index}}
			{{/each}}

			{{#each Reward}}
			{{> _index}}
			{{/each}}

			{{#each Naxxramas}}
			{{> _index}}
			{{/each}}

			{{#each [Goblins vs Gnomes]}}
			{{> _index}}
			{{/each}}

			{{#each [Blackrock Mountain]}}
			{{> _index}}
			{{/each}}

			{{#each [The Grand Tournament]}}
			{{> _index}}
			{{/each}}

			{{#each [The League of Explorers]}}
			{{> _index}}
			{{/each}}

			{{#each [Whispers of the Old Gods]}}
			{{> _index}}
			{{/each}}

			{{#each Karazhan}}
			{{> _index}}
			{{/each}}

			{{#each [Mean Streets of Gadgetzan]}}
			{{> _index}}
			{{/each}}

			<!--

			{{#each [Tavern Brawl]}}
			{{> _index}}
			{{/each}}

			{{#each [Hero Skins]}}
			{{> _index}}
			{{/each}}

			{{#each Missions}}
			{{> _index}}
			{{/each}}


			{{#each Credits}}
			{{> _index}}
			{{/each}}

			{{#each System}}
			{{> _index}}
			{{/each}}


			{{#each Debug}}
			{{> _index}}
			{{/each}}

			-->

		</tbody>
	</table>

{{> pub_footer}}

</div> <!-- End of .container -->
